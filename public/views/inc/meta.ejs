<script>
    (function() {
        var generateTime = time => {
            return time < 10 ? '0' + time : time;
        };
        
        Date.prototype.generateDate = function(){
            return this.getFullYear() + '. ' + generateTime(this.getMonth() + 1) + '. ' + generateTime(this.getDate()) + ' ' + generateTime(this.getHours()) + ':' + generateTime(this.getMinutes()) + ':' + generateTime(this.getSeconds());
        };
    })();
    
</script>